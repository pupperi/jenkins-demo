
docker build -t harbor.workshop.pks101.com/library/golang .

docker run -d -p 8181:8181 harbor.workshop.pks101.com/library/golang

docker login -u admin -p ******** https://harbor.workshop.pks101.com/

docker tag harbor.workshop.pks101.com/library/golang harbor.workshop.pks101.com/library/golang:62

docker push harbor.workshop.pks101.com/library/golang:62

docker tag harbor.workshop.pks101.com/library/golang harbor.workshop.pks101.com/library/golang:latest

docker push harbor.workshop.pks101.com/library/golang:latest

kubectl create -f kubernetes.yml

kubectl get svc golang --output=jsonpath={.status.loadBalancer.ingress[].ip}


